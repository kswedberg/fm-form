{
  "name": "fm-form",
  "version": "1.0.0",
  "description": "Form Components for Fusionary",
  "author": "Karl Swedberg",
  "scripts": {
    "build": "NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:cjs": "NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:iife": "NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:css": "node build/rollup.css.js",
    "prebuild": "npm run build:css",
    "styleguide": "vue-cli-service styleguidist",
    "build:styleguide": "vue-cli-service styleguidist:build"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "dependencies": {
    "@znck/prop-types": "^0.6.3",
    "vue-types": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/cli-service": "^3.10.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-kswedberg": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "minimist": "^1.2.0",
    "node-sass": "^4.12.0",
    "postcss-custom-properties": "^9.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "rollup": "^1.20.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "vue-cli-plugin-styleguidist": "^3.22.2",
    "vue-loader": "^15.7.1",
    "vue-styleguidist": "^3.21.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2"
  },
  "files": [
    "dist/**/*",
    "docs/*",
    "src/**/*.css",
    "src/**/*.js",
    "src/**/*.vue",
    "!src/css.js"
  ],
  "license": "MIT",
  "main": "dist/fm-form.cjs.js",
  "module": "dist/fm-form.js",
  "unpkg": "dist/fm-form.min.js"
}
